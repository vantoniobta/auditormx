<script type="text/javascript">
CFInstall.check({
    mode:"overlay",
    destination: "http://www.google.com/chromeframe?hl=es&prefersystemlevel=true"
  });

$("input[type=checkbox], input[type=radio]").uniform();

String.prototype.basename = function(suffix) {
    var b = this.replace(/^.*[\/\\]/g, '');
    if (typeof(suffix) == 'string' && b.substr(b.length - suffix.length) == suffix) {
      b = b.substr(0, b.length - suffix.length);
    }
    return b;
  }

 
  $(function(){    

    var Apps = new App();
    $(window).resize(Apps.panels);    
      
    var path = window.location.pathname.basename();     
     $('#theNavbar li a').each(function(i,e){
          if( e.href.basename() == path )
            $(this).parents('li').addClass('active');
    });

    // sidebarSize();    
  });



</script>